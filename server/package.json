{
  "name": "express-typescript",
  "version": "1.0.0",
  "private": false,
  "author": "",
  "scripts": {
    "start": "npm run build && npm run serve",
    "test": "npm run build && rimraf tests/sqlite.db && cross-env NODE_ENV=test jest --forceExit --verbose ./dist/tests/users.test.js",
    "build": "npm run clean && tsc",
    "serve": "tsc-watch --outDir ./dist --onSuccess \"node ./dist/app.js\"",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run serve-debug",
    "serve-debug": "tsc-watch --outDir ./dist --onSuccess \"node --inspect=9222 --trace-warnings ./dist/app.js\"",
    "clean": "rimraf dist",
    "sqlz:migrate": "./node_modules/.bin/sequelize db:migrate",
    "sqlz:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "sqlz:new": "./node_modules/.bin/sequelize migration:create",
    "sqlz:seed": "./node_modules/.bin/sequelize db:seed:all",
    "sqlz:seed:create": "node_modules/.bin/sequelize  seed:generate"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.175.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.14.1",
    "compression": "^1.7.1",
    "connect-ensure-login": "^0.1.1",
    "connect-roles": "^3.1.2",
    "cors": "^2.8.4",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "errorhandler": "^1.4.2",
    "express": "^4.13.3",
    "express-limiter": "^1.6.1",
    "html-pdf": "^2.2.0",
    "jsonwebtoken": "^8.1.0",
    "mathjs": "^5.4.0",
    "method-override": "^2.3.5",
    "moment": "^2.23.0",
    "morgan": "^1.6.1",
    "multer": "^1.3.0",
    "nodemailer": "^4.7.0",
    "oauth2orize": "^1.2.0",
    "passport": "^0.4.0",
    "passport-facebook-token": "^3.3.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "passport-http-oauth": "^0.1.3",
    "passport-local": "^1.0.0",
    "passport-oauth2": "^1.1.2",
    "passport-oauth2-client-password": "^0.1.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.3",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "sequelize": "^4.39.0",
    "sequelize-cli": "^4.1.1",
    "sequelize-typescript": "^0.5.0",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/async": "^2.0.40",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/body-parser": "^1.16.2",
    "@types/compression": "0.0.33",
    "@types/errorhandler": "0.0.30",
    "@types/express": "^4.0.35",
    "@types/jest": "^22.0.1",
    "@types/jsonwebtoken": "^7.2.4",
    "@types/lodash": "^4.14.63",
    "@types/morgan": "^1.7.32",
    "@types/multer": "^1.3.6",
    "@types/node": "^8.0.53",
    "@types/passport": "^0.3.3",
    "@types/request": "0.0.45",
    "@types/sequelize": "^4.0.79",
    "@types/supertest": "^2.0.4",
    "jest": "^22.0.6",
    "passport-debug": "^0.1.15",
    "sqlite3": "^4.0.2",
    "supertest": "^3.0.0",
    "ts-node": "^3.3.0",
    "tsc-watch": "^1.0.29",
    "tslint": "^5.0.0",
    "typescript": "^2.6.1"
  }
}
